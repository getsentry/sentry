{
  "_fingerprinting_rules": [
    {
      "matchers": [
        ["value", "*went wrong*"]
      ],
      "fingerprint": ["something-went-wrong"]
    }
  ],
  "fingerprint": ["{{ default }}"],
  "exception": {
    "values": [
      {
        "stacktrace": {
          "frames": [
            {
              "function": "main",
              "abs_path": "Application.java",
              "module": "io.sentry.example.Application",
              "filename": "Application.java",
              "lineno": 13,
              "in_app": false
            }
          ]
        },
        "type": "EndOfWorld",
        "value": "something went wrong"
      }
    ]
  },
  "platform": "java"
}
