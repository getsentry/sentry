{
    "_fingerprinting_rules": [
        {
            "matchers": [
                [
                    "message",
                    "\\{\\[\\*\\?\\]\\}"
                ]
            ],
            "fingerprint": [
                "escaped",
                "{{ message }}"
            ]
        }
    ],
    "logentry": {
        "formatted": "{[*?]}"
    }
}
