{
  "_fingerprinting_rules": [
    {
      "matchers": [
        ["type", "DatabaseUnavailable"]
      ],
      "fingerprint": ["{{ stack.abs_path }}"]
    }
  ],
  "transaction": "my-transaction",
  "exception": {
    "values": [
      {
        "stacktrace": {
          "frames": [
            {
              "function": "main",
              "abs_path": "/foo/Application.cpp",
              "filename": "Application.cpp",
              "lineno": 13,
              "in_app": false
            }
          ]
        },
        "type": "DatabaseUnavailable",
        "value": "For some reason the database went away"
      }
    ]
  },
  "platform": "native"
}
