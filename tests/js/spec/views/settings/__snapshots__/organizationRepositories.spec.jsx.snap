exports[`OrganizationRepositories renders with github provider 1`] = `
<div>
  <SettingsPageHeading
    title="Repositories"
  />
  <AlertLink
    priority="warning"
    to="/settings/org-slug/integrations/"
  >
    Want to add a repository to start tracking commits? Install or configure your version control integration here.
  </AlertLink>
  <div
    className="m-b-2"
  >
    <TextBlock>
      Connecting a repository allows Sentry to capture commit data via webhooks. This enables features like suggested assignees and resolving issues via commit message. Once you've connected a repository, you can associate commits with releases via the API.
       
      <span
        key="5"
      >
        <span
          key="0"
        >
          See our 
        </span>
        <a
          href="https://docs.sentry.io/learn/releases/"
          key="2"
        >
          <span
            key="1"
          >
            documentation
          </span>
        </a>
        <span
          key="3"
        >
           for more details.
        </span>
      </span>
    </TextBlock>
  </div>
  <Panel
    className="align-center p-x-2 p-y-1"
  >
    <Box
      mb={1}
    >
      <HeroIcon
        size="72px"
        src="icon-commit"
      />
    </Box>
    <h3>
      Sentry is better with commit data
    </h3>
    <TextBlock>
      Adding one or more repositories will enable enhanced releases and the ability to resolve Sentry Issues via git message.
    </TextBlock>
    <Box
      mb={1}
    >
      <Button
        disabled={false}
        href="https://docs.sentry.io/learn/releases/"
      >
        Learn more
      </Button>
    </Box>
  </Panel>
</div>
`;

