{
  "version": 2,

  "rewrites": [
    {
      "source": "/(api|organization-avatar|avatar)/(.*)",
      "destination": "https://sentry.io/$1/$2"
    },
    {"source": "/_assets/(.*)", "destination": "/$1"},
    {"source": "/(.*)", "destination": "/index.html"}
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [{"key": "Referer", "value": "https://sentry.io/"}]
    }
  ],
  "github": {"silent": true}
}
