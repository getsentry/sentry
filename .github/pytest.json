{
  "problemMatcher": [
    {
      "owner": "pytest",
      "pattern": [
        {
          "regexp": "^Traceback"
        },
        {
          "regexp": "^\\s+File\\s\"(.*)\",\\sline\\s(\\d+),\\sin\\s(.*)$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": "^\\s+.*"
        },
        {
          "regexp": "^(\\w+Error:.*)",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}
